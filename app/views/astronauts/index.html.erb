<% @astronauts.each do |astro| %>
  <h2><%= astro.name %></h2>
  <h5>Job: <%= astro.job %></h5>
  <h5>Age: <%= astro.age %></h5>
  <h5>Total Time in Space: <%= astro.total_spacetime %></h5>
    <% astro.missions.each do |mission| %>
      <h6><%= mission.title %></h6>

    <% end %>
    <% end %>

<p>Average age for all astronauts: <%= @astronauts.average_age %></p>
